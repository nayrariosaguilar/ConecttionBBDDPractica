<Window x:Class="ConecttionBBDDPractica.SelectClients"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ConecttionBBDDPractica"
        mc:Ignorable="d"
        Title="SelectClients" Height="450" Width="800">
    <Grid x:Name="ListadoClients">
        <DataGrid x:Name="ListadoClientsGrilla" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="128,132,128,178" SelectionChanged="ListadoClientsGrilla_SelectionChanged"/>
    </Grid>
</Window>
